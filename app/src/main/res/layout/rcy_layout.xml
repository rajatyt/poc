<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="169dp"
    android:background="#f6f6f6">


    <TextView
        android:id="@+id/transaction_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/transaction_id_000234568812"
        android:textColor="#0f0f0f"
        android:fontFamily="@font/gibson_medium"
        android:textSize="16dp"
        android:layout_marginTop="19.5dp"
        android:layout_marginStart="16.5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Purchased on Oct 20, 2022"
        android:textColor="#808080"
        android:textSize="12dp"
        android:fontFamily="@font/gibson_regular"
        android:layout_marginTop="40.5dp"
        android:layout_marginStart="16.5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="89dp"
        android:layout_height="16dp"
        android:text="View Details"
        android:textSize="16dp"
        android:layout_marginStart="271dp"
        android:layout_marginTop="31dp"
        android:textColor="#0b4ca1"
        android:fontFamily="@font/gibson_regular"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="71dp"
        android:foregroundGravity="bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_79087" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/child_rcy"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="72.5dp"
        android:layout_marginBottom="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/nested_rcy" />

</androidx.constraintlayout.widget.ConstraintLayout>